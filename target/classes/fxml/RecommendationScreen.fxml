<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mycompany.songbitmaven.RecommendationController">
  <children>
    <Button id="goToSearchButton" fx:id="goToSearch" layoutX="259.0" layoutY="164.0" mnemonicParsing="false" onAction="#goToSearch" text="Search Screen" />
    <Label layoutX="208.0" layoutY="24.0" prefHeight="77.0" prefWidth="184.0" text="RECOMMENDATION PAGE" textAlignment="CENTER">
      <font>
        <Font name="Arial Black" size="12.0" />
      </font>
    </Label>
    <Button fx:id="goToSettings" layoutX="125.0" layoutY="116.0" mnemonicParsing="false" onAction="#goToSettings" text="Settings" />
    <Button fx:id="goToFavorites" layoutX="286.0" layoutY="239.0" mnemonicParsing="false" onAction="#goToFavorites" text="Favorites" />
      <TextField fx:id="login" layoutX="237.0" layoutY="88.0" onAction="#handleLogin" promptText="last.fm username" />
      <Button fx:id="recommend" layoutX="412.0" layoutY="88.0" mnemonicParsing="false" onMouseReleased="#handleRecommend" text="Get Recommendations" />
  </children>
</AnchorPane>
